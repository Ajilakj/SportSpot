   <section class ="profile">
    <div id="profile-header">
        <h3 id="profile-username">{{username}}</h3>
    </div>
    <div id="profile-details">
        <h4>About Me</h4>
        <h5 id="profile-name">{{firstName}} {{lastName}}</h5>
        <p id="profile-bio">{{bio}}</p>
    </div>
    <div id="profile-contact">
        <h4>Contact Info</h4>
        <p id="profile-email">Email: {{email}}</p>
        <p id="profile-phone">Phone: {{phone_number}}</p>
    </div>
   </section>